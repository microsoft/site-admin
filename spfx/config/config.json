{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "site-admin-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/siteAdmin/SiteAdminWebPart.js",
          "manifest": "./src/webparts/siteAdmin/SiteAdminWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "main-lib": {
      "globalName": "main-lib",
      "path": "../dist/site-admin.js"
    }
  },
  "localizedResources": {
    "SiteAdminWebPartStrings": "lib/webparts/siteAdmin/loc/{locale}.js"
  }
}